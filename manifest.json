{
  "name": "dodger",
  "description": "Hide messages",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/"
      ],
      "js": [
        "injector.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dodger.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}